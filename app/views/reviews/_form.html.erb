<%= simple_form_for(@review) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_email %>
    <%= f.input :exp_id %>
    <%= f.input :rating %>
    <%= f.input :rating_desc %>
    <%= f.input :review_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
