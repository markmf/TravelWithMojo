<p id="notice"><%= notice %></p>

<h1>Experiences</h1>

<table>
  <thead>
    <tr>
      <th>Exp email</th>
      <th>Exp</th>
      <th>Exp name</th>
      <th>Exp desc</th>
      <th>Exp provide</th>
      <th>Exp notes</th>
      <th>Exp location</th>
      <th>Image file name</th>
      <th>About me</th>
      <th>Guest reqs</th>
      <th>Max guest</th>
      <th>Rsv guest</th>
      <th>Min guest</th>
      <th>Can policy</th>
      <th>Ratings</th>
      <th>Exp price</th>
      <th>Integer</th>
      <th>Start time</th>
      <th>Duration</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @experiences.each do |experience| %>
      <tr>
        <td><%= experience.exp_email %></td>
        <td><%= experience.exp_id %></td>
        <td><%= experience.exp_name %></td>
        <td><%= experience.exp_desc %></td>
        <td><%= experience.exp_provide %></td>
        <td><%= experience.exp_notes %></td>
        <td><%= experience.exp_location %></td>
        <td><%= experience.image_file_name %></td>
        <td><%= experience.about_me %></td>
        <td><%= experience.guest_reqs %></td>
        <td><%= experience.max_guest %></td>
        <td><%= experience.rsv_guest %></td>
        <td><%= experience.min_guest %></td>
        <td><%= experience.can_policy %></td>
        <td><%= experience.ratings %></td>
        <td><%= experience.exp_price %></td>
        <td><%= experience.integer %></td>
        <td><%= experience.start_time %></td>
        <td><%= experience.duration %></td>
        <td><%= link_to 'Show', experience %></td>
        <td><%= link_to 'Edit', edit_experience_path(experience) %></td>
        <td><%= link_to 'Destroy', experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Experience', new_experience_path %>
